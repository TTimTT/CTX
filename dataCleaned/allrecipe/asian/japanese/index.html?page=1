    <ar-save-item class='favorite' data-id="218877" data-type="'Recipe'" data-name="&quot;The Perfect Simplified Sushi Vinegar&quot;" data-segmentpageproperties="segmentContentInfo" data-imageurl="'https://images.media-allrecipes.com/userphotos/300x300/5192088.jpg'"></ar-save-item>
