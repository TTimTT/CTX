    <ar-save-item class='favorite' data-id="255416" data-type="'Recipe'" data-name="&quot;Okonomiyaki (Japanese Pancake)&quot;" data-segmentpageproperties="segmentContentInfo" data-imageurl="'https://images.media-allrecipes.com/userphotos/300x300/5363444.jpg'"></ar-save-item>
