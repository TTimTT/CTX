    <ar-save-item class='favorite' data-id="25371" data-type="'Recipe'" data-name="&quot;Lime Chicken Soft Tacos&quot;" data-segmentpageproperties="segmentContentInfo" data-imageurl="'https://images.media-allrecipes.com/userphotos/300x300/642313.jpg'"></ar-save-item>
